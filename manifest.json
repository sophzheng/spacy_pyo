{
  "manifest_version": 2,

  "name": "locately spacy",
  "description": "This extension runs Python code (PYO).",
  "version": "1.0",

  "browser_action": {

    "default_popup": "popup_pyo.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline' https://cdn.jsdelivr.net/pyodide/v0.21.0a2/full/pyodide.js https://cdn.jsdelivr.net/pyodide/v0.21.0a2/full/pyodide.asm.js",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "<all_urls>"
  ]
}
